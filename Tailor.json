{
    "rules": [
        {
            "match": "FATAL",
            "color": "Red"
        },
        {
            "match": "WARNING",
            "color": "Yellow"
        },
        {
            "match": "DEBUG",
            "color": "Gray"
        }
    ],
    "default": {
        "enabled": true,
        "color": "White"
    }
}